{
  "name": "template-mock-bot",
  "version": "0.0.0-0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "iisnode.yml",
    "dist/**/*",
    "web.config"
  ],
  "main": "./dist/index.js",
  "type": "module",
  "tsup": {
    "format": "esm",
    "target": "node18"
  },
  "scripts": {
    "build": "tsup-node ./src/index.ts",
    "precommit": "eslint .",
    "start": "nodemon --exec \"npm run build && node .\" --ext js,json,ts --verbose --watch './src/**'"
  }
}
